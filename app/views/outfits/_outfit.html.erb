<%= form_for @outfit do |f| %>
<p>
  <%= f.label :title %>
  <%= f.text_field :title, :autofocus => true, :required => true %>
</p>
<p>
  <%= f.label :description %>
  <%= f.text_field :description %>
</p>
<p>
  <%= f.label :image %>
  <%= f.text_field :image, :required => true %>
</p>
<p>
  <%#= f.label :tag %>
  <%#= f.text_field :tag %>
</p>
<p>
  <%= f.submit %>
</p>

<% end %>